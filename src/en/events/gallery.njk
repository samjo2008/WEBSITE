---
layout: layouts/base.njk
lang: en
title: "Meskel Photo Album"
permalink: "en/events/gallery/index.html"
---

<section class="album-container">

  <h1 class="album-page-title">Meskel Photo Album</h1>

  <div class="album-hero">
    <img id="heroImage-meskel" src="{{ collections.meskelGallery[0] }}" alt="Meskel Hero">
    <span class="album-prev" data-target="meskel">&#10094;</span>
    <span class="album-next" data-target="meskel">&#10095;</span>
  </div>

  <div class="album-thumbnails" data-target="meskel">
    {% for img in collections.meskelGallery %}
    <img 
      src="{{ img }}" 
      class="thumb thumb-meskel"
      data-full="{{ img }}"
      alt="Thumbnail">
    {% endfor %}
  </div>

</section>



